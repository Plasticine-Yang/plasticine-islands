{
  "private": true,
  "author": "Plasticine-Yang",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start:cli": "pnpm -C packages/cli start",
    "build": "pnpm build:cli & pnpm build:core & pnpm build:vite-plugin-dev-server-html",
    "build:cli": "pnpm -C packages/cli build",
    "build:core": "pnpm -C packages/core build",
    "build:vite-plugin-dev-server-html": "pnpm -C packages/vite-plugin-dev-server-html build",
    "stub": "pnpm stub:cli & pnpm stub:core & pnpm stub:vite-plugin-dev-server-html",
    "stub:cli": "pnpm -C packages/cli stub",
    "stub:core": "pnpm -C packages/core stub",
    "stub:vite-plugin-dev-server-html": "pnpm -C packages/vite-plugin-dev-server-html stub",
    "test": "vitest",
    "test:u": "vitest -u",
    "play:dev": "pnpm -C playground dev",
    "play:build": "pnpm -C playground build",
    "commit": "cz",
    "up": "taze -wI",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@plasticine-yang/eslint-config-typescript": "^0.1.6",
    "@types/node": "^18.14.1",
    "commitizen": "^4.3.0",
    "cz-git": "^1.5.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "taze": "^0.8.5",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vitest": "^0.28.5"
  }
}
