{
  "name": "@plasticine-islands/vite-plugin-plasticine-islands-site-config",
  "version": "0.0.3",
  "author": "Plasticine-Yang",
  "description": "An internal vite plugin to supply virtual module of site configuration for @plasticine-islands packages.",
  "type": "module",
  "license": "MIT",
  "files": [
    "./dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "vite plugin"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --dts"
  },
  "dependencies": {
    "@plasticine-islands/types": "workspace:*",
    "chalk": "^5.2.0"
  },
  "peerDependencies": {
    "vite": "^4.1.4"
  },
  "devDependencies": {
    "vite": "^4.1.4"
  }
}
