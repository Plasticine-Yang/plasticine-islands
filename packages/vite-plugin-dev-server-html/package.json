{
  "name": "@plasticine-islands/vite-plugin-dev-server-html",
  "version": "0.0.3",
  "author": "Plasticine-Yang",
  "description": "A vite plugin to load html for dev server.",
  "type": "module",
  "files": [
    "./dist"
  ],
  "license": "MIT",
  "main": "./dist/main/index.cjs",
  "module": "./dist/main/index.js",
  "types": "./dist/main/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/main/index.cjs",
      "import": "./dist/main/index.js",
      "types": "./dist/main/index.d.ts"
    }
  },
  "keywords": [
    "vite plugin",
    "dev server plugin",
    "load html"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --dts"
  },
  "dependencies": {
    "@plasticine-islands/types": "workspace:*"
  },
  "peerDependencies": {
    "vite": "^4.1.4"
  },
  "devDependencies": {
    "vite": "^4.1.4"
  }
}
